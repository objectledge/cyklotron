CREATE RESOURCE 'sitemaps'
RESOURCE CLASS 'cms.sitemap.configuration'
PARENT '/cms'
ATTRIBUTES (
  providerConfig = '@empty',
  compress = 'false'
);

CREATE RESOURCE 'sitemap'
RESOURCE CLASS 'integration.application'
PARENT '/cms/applications'
ATTRIBUTES (
  applicationName = 'cms',
  vendor = 'caltha.pl',
  version = '1.0',
  priority = 5,
  enabled = true,
  required = false
);