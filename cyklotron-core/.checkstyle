<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.0.0">
    <fileset name="main sources" enabled="true" check-config-name="Ledge checks">
        <file-match-pattern match-pattern="main.*java$" include-pattern="true"/>
    </fileset>
    <fileset name="test sources" enabled="true" check-config-name="Ledge test checks">
        <file-match-pattern match-pattern="test.*java$" include-pattern="true"/>
    </fileset>
</fileset-config>
